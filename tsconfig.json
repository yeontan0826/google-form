{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@stacks/*": ["./src/stacks/*"],
      "@redux/*": ["./src/redux/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@assets/*": ["./assets/*"],
      "@/*": ["./*"]
    }
  }
}
